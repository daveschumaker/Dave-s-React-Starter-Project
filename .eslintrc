{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "defaults/configurations/airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": 1,
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1, 
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": [2, "single"],
    "jsx-quotes": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  }
}